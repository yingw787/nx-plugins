{
  "version": 1,
  "projects": {
    "nx-deploy-it": {
      "root": "libs/nx-deploy-it",
      "sourceRoot": "libs/nx-deploy-it/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/nx-deploy-it/.eslintrc",
            "tsConfig": [
              "libs/nx-deploy-it/tsconfig.lib.json",
              "libs/nx-deploy-it/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/nx-deploy-it/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/nx-deploy-it/jest.config.js",
            "tsConfig": "libs/nx-deploy-it/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/nx-deploy-it",
            "tsConfig": "libs/nx-deploy-it/tsconfig.lib.json",
            "packageJson": "libs/nx-deploy-it/package.json",
            "main": "libs/nx-deploy-it/src/index.ts",
            "assets": [
              "libs/nx-deploy-it/*.md",
              {
                "input": "./libs/nx-deploy-it/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./libs/nx-deploy-it",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./libs/nx-deploy-it",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "nx-deploy-it-e2e": {
      "projectType": "application",
      "root": "apps/nx-deploy-it-e2e",
      "sourceRoot": "apps/nx-deploy-it-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "nx-deploy-it:build",
            "npmPackageName": "@dev-thought/nx-deploy-it",
            "pluginOutputPath": "dist/libs/nx-deploy-it",
            "jestConfig": "apps/nx-deploy-it-e2e/jest.config.js",
            "tsSpecConfig": "apps/nx-deploy-it-e2e/tsconfig.spec.json"
          }
        }
      }
    },
    "test-app": {
      "root": "apps/test-app",
      "sourceRoot": "apps/test-app",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/test-app",
            "outputPath": "dist/apps/test-app"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/test-app/environments/environment.ts",
                  "with": "apps/test-app/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/next:dev-server",
          "options": {
            "buildTarget": "test-app:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "test-app:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "test-app:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/test-app/.eslintrc",
            "tsConfig": [
              "apps/test-app/tsconfig.json",
              "apps/test-app/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/test-app/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/test-app/jest.config.js",
            "tsConfig": "apps/test-app/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "test-app-e2e": {
      "root": "apps/test-app-e2e",
      "sourceRoot": "apps/test-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/test-app-e2e/cypress.json",
            "tsConfig": "apps/test-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "test-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "test-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/test-app-e2e/.eslintrc",
            "tsConfig": ["apps/test-app-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/test-app-e2e/**"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint",
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "test-app"
}
